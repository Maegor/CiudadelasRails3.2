
<%if current_user.player.nil? || current_user.player.party.state.nil?%>
    $('#table_container').html("<%=escape_javascript (render :partial => 'users_table') %>")
    <% if @room.users.count == @room.user  && current_user.id == @room.owner_id%>


    var start = $('#start');
   if  (start.length() > 0){
       start.html("<%=escape_javascript(render :partial=> 'start_button')%>");
   }else{

       start.append("<%=escape_javascript(render :partial=> 'start_button')%>");

   }
    <%end%>

<%else%>


function enter_game(){
 window.location.replace("<%= party_path(current_user.player.party.id) %>");
 window.location = "<%=party_path(current_user.player.party.id) %>"
 }
 enter_game();

<%end%>